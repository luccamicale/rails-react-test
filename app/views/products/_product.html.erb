<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= product.description %>
  </p>

  <p>
    <strong>Icon:</strong>
    <%= product.icon %>
  </p>

  <p>
    <strong>Brand:</strong>
    <%= product.brand %>
  </p>

</div>
